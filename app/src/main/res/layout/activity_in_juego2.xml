<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@drawable/img_fondo">

    <!--header-->
    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/imageView12"
        app:srcCompat="@drawable/img_pleca_verde"
        tools:ignore="ContentDescription" />

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/imageView"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="45dp"
        app:srcCompat="@drawable/img_pleca_roja"
        tools:ignore="ContentDescription" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="280dp"
        android:layout_height="132dp"
        android:layout_alignBottom="@+id/imageView13"
        android:layout_alignEnd="@+id/imageView15"
        android:layout_marginEnd="39dp"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/img_in_dialogo"
        tools:ignore="ContentDescription" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:scaleType="fitXY"
        android:src="@drawable/img_fonacot"
        tools:ignore="ContentDescription" />

    <ImageButton
        android:id="@+id/btn_home"
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:layout_above="@+id/imageView15"
        android:layout_centerHorizontal="true"
        android:background="@null"
        android:scaleType="fitXY"
        android:src="@drawable/btn_home"
        tools:ignore="ContentDescription" />

    <ImageView
        android:id="@+id/imageView13"
        android:layout_width="100dp"
        android:layout_height="180dp"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/img_nina"
        tools:ignore="ContentDescription"
        android:layout_alignTop="@+id/imageView_p5"
        android:layout_toStartOf="@+id/imageView11" />

    <ImageView
        android:id="@+id/imageView15"
        android:layout_width="550dp"
        android:layout_height="10dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="40dp"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/img_plecamini_verde"
        tools:ignore="ContentDescription" />

    <ImageView
        android:id="@+id/imageView16"
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:layout_above="@+id/imageView14"
        android:layout_alignParentEnd="true"
        android:background="@null"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/img_in_icono"
        tools:ignore="ContentDescription" />

    <ImageView
        android:id="@+id/imageView14"
        android:layout_width="550dp"
        android:layout_height="10dp"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/imageView15"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/img_plecamini_roja"
        tools:ignore="ContentDescription" />

    <ImageView
        android:id="@+id/imageView_c2"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:tag="imageView_c2"
        app:srcCompat="@drawable/btn_in_cuadro2"
        tools:ignore="ContentDescription"
        android:layout_below="@+id/imageView_p2"
        android:layout_alignStart="@+id/imageView_c1" />

    <ImageView
        android:id="@+id/imageView_c4"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_alignBottom="@+id/imageView11"
        android:layout_alignStart="@+id/imageView_c3"
        android:tag="imageView_c4"
        app:srcCompat="@drawable/btn_in_cuadro4"
        tools:ignore="ContentDescription" />

    <ImageView
        android:id="@+id/imageView_c1"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:tag="imageView_c1"
        app:srcCompat="@drawable/btn_in_cuadro1"
        tools:ignore="ContentDescription"
        android:layout_below="@+id/imageView_p1"
        android:layout_alignStart="@+id/btn_home" />

    <ImageView
        android:id="@+id/imageView_c3"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:tag="imageView_c3"
        app:srcCompat="@drawable/btn_in_cuadro3"
        tools:ignore="ContentDescription"
        android:layout_below="@+id/imageView_p3"
        android:layout_alignStart="@+id/imageView_c2" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="400dp"
        android:layout_height="247dp"
        app:srcCompat="@drawable/img_in_cuadro0"
        tools:ignore="ContentDescription"
        android:layout_centerVertical="true"
        android:layout_toStartOf="@+id/imageView" />

    <ImageView
        android:id="@+id/imageView19"
        android:layout_width="390dp"
        android:layout_height="450dp"
        android:layout_alignStart="@+id/imageView4"
        android:layout_below="@+id/imageView4"
        app:srcCompat="@drawable/img_in_cuadro2"
        tools:ignore="ContentDescription" />

    <ImageView
        android:id="@+id/imageView_p1"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:tag="imageView_p1"
        app:srcCompat="@drawable/btn_in_contenedor"
        tools:ignore="ContentDescription"
        android:layout_below="@+id/imageView"
        android:layout_toStartOf="@+id/imageView19" />

    <ImageView
        android:id="@+id/imageView_p2"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_below="@+id/imageView_p1"
        android:layout_marginTop="31dp"
        android:layout_toStartOf="@+id/imageView19"
        android:tag="imageView_p2"
        app:srcCompat="@drawable/btn_in_contenedor"
        tools:ignore="ContentDescription" />

    <ImageView
        android:id="@+id/imageView_p3"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginTop="29dp"
        android:tag="imageView_p3"
        app:srcCompat="@drawable/btn_in_contenedor"
        tools:ignore="ContentDescription"
        android:layout_below="@+id/imageView_p2"
        android:layout_toStartOf="@+id/imageView19" />

    <ImageView
        android:id="@+id/imageView_p4"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_below="@+id/imageView_p3"
        android:layout_marginTop="31dp"
        android:layout_toStartOf="@+id/imageView19"
        android:tag="imageView_p4"
        app:srcCompat="@drawable/btn_in_contenedor"
        tools:ignore="ContentDescription" />

    <ImageView
        android:id="@+id/imageView_p5"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_below="@+id/imageView_p4"
        android:layout_marginTop="31dp"
        android:layout_toStartOf="@+id/imageView19"
        android:tag="imageView_p5"
        app:srcCompat="@drawable/btn_in_contenedor"
        tools:ignore="ContentDescription" />

    <ImageView
        android:id="@+id/imageView_p6"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_below="@+id/imageView_p5"
        android:layout_marginTop="31dp"
        android:layout_toStartOf="@+id/imageView19"
        android:tag="imageView_p6"
        app:srcCompat="@drawable/btn_in_contenedor"
        tools:ignore="ContentDescription" />

    <ImageButton
        android:id="@+id/btn_anterior"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_centerVertical="true"
        android:background="@null"
        android:contentDescription=""
        android:src="@drawable/btn_atras"
        tools:ignore="ContentDescription" />

</RelativeLayout>




