        android:layout_height="120dp"
        android:layout_alignStart="@+id/imageButton_o3"
        android:layout_alignTop="@+id/imageButton_o5"
        android:background="@null"
        android:scaleType="fitXY"
        android:tag="img_pr_casilla6_uf"
        app:srcCompat="@drawable/img_pr_casilla6_uf"
        tools:ignore="ContentDescription" />

    <ImageButton
        android:id="@+id/imageButton_o3"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:background="@null"
        android:scaleType="fitXY"
        android:tag="img_pr_casilla3_uf"
        app:srcCompat="@drawable/img_pr_casilla3_uf"
        tools:ignore="ContentDescription"
        android:layout_above="@+id/imageButton_o6"
        android:layout_toEndOf="@+id/imageButton_o2" />

    <ImageButton
        android:id="@+id/imageButton_o2"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:background="@null"
        android:scaleType="fitXY"
        android:tag="img_pr_casilla2_uf"
        app:srcCompat="@drawable/img_pr_casilla2_uf"
        tools:ignore="ContentDescription"
        android:layout_alignTop="@+id/imageButton_o1"
        android:layout_toEndOf="@+id/imageButton_o4" />

    <ImageButton
        android:id="@+id/imageButton_o5"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_alignStart="@+id/imageButton_o2"
        android:layout_below="@+id/imageButton_o2"
        android:background="@null"
        android:scaleType="fitXY"
        android:tag="img_pr_casilla5_uf"
        app:srcCompat="@drawable/img_pr_casilla5_uf"
        tools:ignore="ContentDescription" />

    <ImageButton
        android:id="@+id/imageButton_o4"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_alignStart="@+id/imageButton_o1"
        android:layout_below="@+id/imageButton_o1"
        android:background="@null"
        android:scaleType="fitXY"
        android:tag="img_pr_casilla4_uf"
        app:srcCompat="@drawable/img_pr_casilla4_uf"
        tools:ignore="ContentDescription" />

    <ImageButton
        android:id="@+id/imageButton_o9"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:background="@null"
        android:scaleType="fitXY"
        android:tag="img_pr_casilla9_uf"
        app:srcCompat="@drawable/img_pr_casilla9_uf"
        tools:ignore="ContentDescription"
        android:layout_above="@+id/imageButton_o12"
        android:layout_alignEnd="@+id/imageView9"
        android:layout_marginEnd="27dp" />

    <ImageButton
        android:id="@+id/imageButton_o8"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:background="@null"
        android:scaleType="fitXY"
        android:tag="img_pr_casilla8_uf"
        app:srcCompat="@drawable/img_pr_casilla8_uf"
        tools:ignore="ContentDescription"
        android:layout_alignTop="@+id/imageButton_o7"
        android:layout_toStartOf="@+id/imageButton_o9" />

    <ImageButton
        android:id="@+id/imageButton_o7"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:background="@null"
        android:scaleType="fitXY"
        android:tag="img_pr_casilla7_uf"
        app:srcCompat="@drawable/img_pr_casilla7_uf"
        tools:ignore="ContentDescription"
        android:layout_above="@+id/imageButton_o10"
        android:layout_toStartOf="@+id/imageButton_o11" />

    <ImageButton
        android:id="@+id/imageButton_o1"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_alignStart="@+id/imageView5"
        android:layout_below="@+id/imageView5"
        android:layout_marginTop="29dp"
        android:background="@null"
        android:scaleType="fitXY"
        android:tag="img_pr_casilla1_uf"
        app:srcCompat="@drawable/img_pr_casilla1_uf"
        tools:ignore="ContentDescription" />

    <ImageButton
        android:id="@+id/imageButton_o12"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:background="@null"
        android:scaleType="fitXY"
        android:tag="img_pr_casilla12_uf"
        app:srcCompat="@drawable/img_pr_casilla12_uf"
        tools:ignore="ContentDescription"
        android:layout_below="@+id/imageButton_o8"
        android:layout_alignStart="@+id/imageButton_o9" />

    <ImageButton
        android:id="@+id/imageButton_o11"
        android:layout_width="120dp"
