    <ImageButton
        android:id="@+id/imageButton_o10"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:background="@null"
        android:scaleType="fitXY"
        android:tag="img_pr_casilla10_uf"
        app:srcCompat="@drawable/img_pr_casilla10_uf"
        tools:ignore="ContentDescription"
        android:layout_below="@+id/imageButton_o3"
        android:layout_alignStart="@+id/imageButton_o7" />

    <ImageButton
        android:id="@+id/btn_siguiente"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_centerVertical="true"
        android:background="@null"
        android:contentDescription=""
        android:src="@drawable/btn_siguiente"
        tools:ignore="ContentDescription" />


</RelativeLayout>
